<!DOCTYPE html>
<html lang="en">
<%- include header %>
<body>
    <div id="navigator">
        <div class="logo">
            <a href="/panel">
                <em class="nl1"></em>
                <em class="nl2"></em>
                <em class="nl3"></em>
                <i class="material-icons">slow_motion_video</i>
            </a>
        </div>
        <div class="navigator">
            <ul>
                <li>
                    <a href="/panel/system">
                        <em class="nl1"></em>
                        <em class="nl2"></em>
                        <em class="nl3"></em>
                        <i></i>
                        <span>系统</span>
                    </a>
                </li>
                <li>
                   <a href="/panel/contents">
                        <em class="nl1"></em>
                        <em class="nl2"></em>
                        <em class="nl3"></em>
                        <i></i>
                        <span>内容</span>
                   </a>
                </li>
                <li>
                   <a href="/panel/users">
                        <em class="nl1"></em>
                        <em class="nl2"></em>
                        <em class="nl3"></em>
                        <i></i>
                        <span>用户</span>
                   </a>
                </li>
                <li>
                   <a href="/panel/styles">
                        <em class="nl1"></em>
                        <em class="nl2"></em>
                        <em class="nl3"></em>
                        <i></i>
                        <span>样式</span>
                   </a>
                </li>
                <li>
                   <a href="/panel/extensions">
                        <em class="nl1"></em>
                        <em class="nl2"></em>
                        <em class="nl3"></em>
                        <i></i>
                        <span>拓展</span>
                   </a>
                </li>
            </ul>
        </div>
    </div>
    
<% include brand %>
    
    <div class="container">
        <div class="index_news metro">
            <div class="pd20">
                <h4 class="blue fl">新消息</h4>
                <a href="http://www.quickcms.cn" target="_blank" class="more fr"><i class="material-icons">link</i><span>更多</span></a>
                <div class="clear"></div>
                <ul>
                    <% 
                    var news = infomations.news || [];
                    news.forEach(function(d){ %>
                        <li><a target="_blank" href="<%= d.url %>"><%= d.title %></a><span>(<%= d.postdate %>)</span></li>
                    <% });
                    %>
                </ul>
            </div>
        </div>
        <div class="index_system metro">
            <div class="pd20">
                <h4 class="blue">系统信息</h4>
                <ul>
                    <li><em>网站版本</em><span> <%= infomations.versions %> </span></li>
                    <li><em>系统版本</em><span><%= infomations.systemver %></span></li>
                    <li><em>IP地址</em><span><%= infomations.address %></span></li>
                    <li><em>运行时间</em><span class="uptime"><%= infomations.date.days %>天<%= infomations.date.hours %>小时<%= infomations.date.minutes %>分<%= infomations.date.seconds %>秒</span></li>
                </ul>
            </div>
        </div>
        <div class="index_memory metro">
            <div class="pd20">
                <h4 class="blue">内存使用</h4>
                <span><em style="width:<%= infomations.freemem/infomations.totalmem*100 %>%"></em></span>
                <div class="memory_rate">
                   <b><%= infomations.freemem %></b> / <%= infomations.totalmem %> (<%= Math.ceil( (infomations.freemem/infomations.totalmem) * 100 ) + '%' %>)
                </div>
            </div>
        </div>
        <div class="index_cpu metro">
            <div class="pd20">
                <h4 class="blue">CPU信息</h4>
                <ul>
                    <li><em>CPU架构</em><span><%= infomations.arch %></span></li>
                    <li><em>CPU线程</em><span><%= infomations.thread %></span></li>
                    <li><em>详细信息</em></li>
                </ul>
                <p>
                    <%= infomations.cpus.model %><br />
                    Speed : <%= infomations.cpus.speed %>MHZ<br />
                    user : <%= infomations.cpus.times.user %>  nice : <%= infomations.cpus.times.nice %>  sys : <%= infomations.cpus.times.sys %>
                    <br />
                    idle : <%= infomations.cpus.times.idle %>  irq : <%= infomations.cpus.times.irq %>
                </p>
            </div>
        </div>
        <div class="index_avg metro">
            <div class="pd20">
                <h4 class="blue">平均负载</h4>
                <canvas id="drawavg"></canvas>
                <div class="path"></div>
            </div>
        </div>
    </div>
    
    <script>
        var uptime = <%- infomations.uptime %>
    </script>
    <%- include footer %>
</body>
</html>